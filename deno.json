{
  "name": "@cyclonedx/cdxgen",
  "version": "12.1.0",
  "exports": "./lib/cli/index.js",
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  },
  "include": ["*.js", "data/", "bin/", "lib/**"],
  "exclude": [
    "test/",
    "docs/",
    "contrib/",
    "ci/",
    "tools_config/",
    ".github/",
    ".*",
    "devenv*",
    "jest.config.js",
    "docker-compose.yml",
    "ADVANCED.md",
    "pnpm-lock.yaml",
    "biome.json",
    "plugins",
    "tsconfig.json",
    "index.cjs"
  ],
  "compile": {
    "include": ["data", "package.json"]
  },
  "lint": {
    "include": ["bin/", "*.js", "lib/**"],
    "exclude": ["test/", "data/", "contrib/"],
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-unused-vars", "no-invalid-regexp"]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 4,
    "semiColons": false,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["bin/", "*.js"],
    "exclude": ["test/", "data/", "contrib/"]
  },
  "lock": false,
  "nodeModulesDir": "auto",
  "unstable": [],
  "test": {
    "include": ["*.poku.js"]
  },
  "tasks": {
    "start": "deno run --allow-read --allow-env --allow-run --allow-sys=uid,systemMemoryInfo,gid,homedir --allow-write --allow-net bin/cdxgen.js",
    "exe": "deno compile --allow-read --allow-env --allow-run --allow-sys=uid,systemMemoryInfo,gid,homedir --allow-write --allow-net --output build/cdxgenx-devel bin/cdxgen.js",
    "lint:check": "biome check *",
    "lint": "biome check --apply-unsafe *",
    "gen-types": "npx -p typescript tsc"
  },
  "imports": {
    "encoding": "npm:encoding@0.1.13",
    "yaml": "npm:yaml@2.8.1",
    "@isaacs/string-locale-compare": "npm:@isaacs/string-locale-compare@1.1.0",
    "@npmcli/fs": "npm:@npmcli/fs@4.0.0",
    "@npmcli/installed-package-contents": "npm:@npmcli/installed-package-contents@3.0.0",
    "@npmcli/map-workspaces": "npm:@npmcli/map-workspaces@5.0.0",
    "@npmcli/name-from-folder": "npm:@npmcli/name-from-folder@3.0.0",
    "@npmcli/package-json": "npm:@npmcli/package-json@7.0.0",
    "@npmcli/query": "npm:@npmcli/query@4.0.1",
    "@npmcli/redact": "npm:@npmcli/redact@3.2.2",
    "bin-links": "npm:bin-links@5.0.0",
    "common-ancestor-path": "npm:common-ancestor-path@1.0.1",
    "hosted-git-info": "npm:hosted-git-info@9.0.0",
    "json-stringify-nice": "npm:json-stringify-nice@1.1.4",
    "minimatch": "npm:minimatch@10.0.3",
    "npm-install-checks": "npm:npm-install-checks@7.1.2",
    "npm-package-arg": "npm:npm-package-arg@13.0.0",
    "npm-pick-manifest": "npm:npm-pick-manifest@10.0.0",
    "parse-conflict-json": "npm:parse-conflict-json@4.0.0",
    "proc-log": "npm:proc-log@5.0.0",
    "proggy": "npm:proggy@3.0.0",
    "promise-all-reject-late": "npm:promise-all-reject-late@1.0.1",
    "promise-call-limit": "npm:promise-call-limit@3.0.2",
    "read-package-json-fast": "npm:read-package-json-fast@4.0.0",
    "treeverse": "npm:treeverse@3.0.0",
    "walk-up-path": "npm:walk-up-path@4.0.0",
    "@appthreat/atom": "npm:@appthreat/atom@2.3.0",
    "@appthreat/cdx-proto": "npm:@appthreat/cdx-proto@1.1.4",
    "@bufbuild/protobuf": "npm:@bufbuild/protobuf@2.6.3",
    "@babel/parser": "npm:@babel/parser@^7.28.4",
    "@babel/traverse": "npm:@babel/traverse@^7.28.4",
    "ajv": "npm:ajv@^8.16.0",
    "ajv-formats": "npm:ajv-formats@^3.0.1",
    "cheerio": "npm:cheerio@^1.1.2",
    "edn-data": "npm:edn-data@1.1.2",
    "glob": "npm:glob@^11.0.3",
    "global-agent": "npm:global-agent@^3.0.0",
    "got": "npm:got@^14.4.8",
    "iconv-lite": "npm:iconv-lite@^0.7.0",
    "js-yaml": "npm:js-yaml@^4.1.0",
    "jws": "npm:jws@^4.0.0",
    "node-stream-zip": "npm:node-stream-zip@^1.15.0",
    "packageurl-js": "npm:packageurl-js@1.0.2",
    "prettify-xml": "npm:prettify-xml@^1.2.0",
    "properties-reader": "npm:properties-reader@^2.3.0",
    "semver": "npm:semver@^7.7.2",
    "ssri": "npm:ssri@^12.0.0",
    "table": "npm:table@^6.9.0",
    "tar": "npm:tar@^7.4.3",
    "uuid": "npm:uuid@^11.1.0",
    "xml-js": "npm:xml-js@^1.6.11",
    "yargs": "npm:yargs@^18.0.0",
    "yoctocolors": "npm:yoctocolors@^2.1.2",
    "sequelize": "npm:sequelize@^6.37.7",
    "sqlite3": "npm:@appthreat/sqlite3@^6.0.9",
    "body-parser": "npm:body-parser@^2.2.0",
    "compression": "npm:compression@^1.8.1",
    "connect": "npm:connect@^3.7.0",
    "jsonata": "npm:jsonata@^2.1.0",
    "@iarna/toml": "npm:@iarna/toml@2.2.5"
  }
}
