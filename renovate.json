    "config:recommended",
    ":separateMultipleMajorReleases",
    ":enableVulnerabilityAlerts",
    "schedule:monthly",
    "customManagers:biomeVersions",
    "helpers:pinGitHubActionDigests"
  ],
  "labels": ["dependency"],
  "rangeStrategy": "pin",
  "enabledManagers": ["github-actions", "npm", "nvm"]
}
