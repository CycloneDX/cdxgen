  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/cli/index.js",
      "types": "./types/lib/cli/index.d.ts",
      "require": "./index.cjs"
    },
    "./evinser/*": {
      "import": "./lib/evinser/*.js",
      "types": "./types/lib/evinser/*.d.ts"
    },
    "./helpers/*": {
      "import": "./lib/helpers/*.js",
      "types": "./types/lib/helpers/*.d.ts"
    },
    "./managers/*": {
      "import": "./lib/managers/*.js",
      "types": "./types/lib/managers/*.d.ts"
    },
    "./parsers/*": {
      "import": "./lib/parsers/*.js",
      "types": "./types/lib/parsers/*.d.ts"
    },
    "./stages/postgen/*": {
      "import": "./lib/stages/postgen/*.js",
      "types": "./types/lib/stages/postgen/*.d.ts"
    },
    "./stages/pregen/*": {
      "import": "./lib/stages/pregen/*.js",
      "types": "./types/lib/stages/pregen/*.d.ts"
    }
  },
  "types": "./types/lib/cli/index.d.ts",
  "bin": {
    "cbom": "bin/cdxgen.js",
    "cdx-verify": "bin/verify.js",
    "cdxgen": "bin/cdxgen.js",
    "cdxgen-secure": "bin/cdxgen.js",
    "cdxi": "bin/repl.js",
    "evinse": "bin/evinse.js",
    "obom": "bin/cdxgen.js",
    "saasbom": "bin/cdxgen.js"
  },
  "files": [
    "*.js",
    "lib/**",
    "bin/",
    "data/",
    "types/",
    "index.cjs"
  ],
  "scripts": {
    "gen-types": "npx -p typescript tsc",
    "install:frozen": "pnpm install --config.strict-dep-builds=true --frozen-lockfile --package-import-method copy",
    "install:prod": "pnpm install --config.strict-dep-builds=true --frozen-lockfile --package-import-method copy --prod",
    "lint": "biome check --fix",
    "lint:check": "biome check",
    "lint:errors": "biome check --diagnostic-level=error",
    "test": "poku",
    "watch": "poku --watch"
  },
  "lint-staged": {
    "*": "biome check --fix --no-errors-on-unmatched"
  },
